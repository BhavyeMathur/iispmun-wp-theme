{
    "version": 2,
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "settings": {
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color": {
            "link": true,
            "text": true,
            "background": true,

            "customGradient": false,
            "customDuotone": false,
            "custom": false,

            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,

            "palette": [
                {
                    "name": "Background",
                    "slug": "background",
                    "color": "#ffffff"
                },
                {
                    "name": "Foreground",
                    "slug": "foreground",
                    "color": "#000000"
                },
                {
                    "name": "Paragraph",
                    "slug": "paragraph",
                    "color": "#aaaaaa"
                },
                {
                    "name": "Darkest Purple",
                    "slug": "darkest-purple",
                    "color": "#121219"
                },
                {
                    "name": "Dark Purple",
                    "slug": "dark-purple",
                    "color": "#20202E"
                },
                {
                    "name": "Purple",
                    "slug": "purple",
                    "color": "#3C3C4F"
                },
                {
                    "name": "Light Purple",
                    "slug": "light-purple",
                    "color": "#71718D"
                },
                {
                    "name": "Lighest Purple",
                    "slug": "lightest-purple",
                    "color": "#A8A8BF"
                },
                {
                    "name": "Primary",
                    "slug": "primary",
                    "color": "#74B461"
                },
                {
                    "name": "Secondary",
                    "slug": "secondary",
                    "color": "#FFCC66"
                }
            ]
        },
        "spacing": {
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ],
            "blockGap": null,
            "margin": true,
            "padding": true,
            "customSpacingSize": true
        },
        "layout": {
            "contentSize": "840px",
            "wideSize": "1200px"
        },
        "typography": {
            "dropCap": true,
            "lineHeight": true,
            "letterSpacing": true,
            "textDecoration": true,
            "textTransform": true,
            "textColumns": true,
            "customFontSize": false,
            "fontStyle": true,
            "fontWeight": true,

            "fontFamilies": [
                {
                    "name": "System",
                    "slug": "system",
                    "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
                }
            ],
            "fontSizes": [
                {
                    "slug": "extra-small",
                    "size": "0.8rem",
                    "name": "Extra small"
                },
                {
                    "slug": "small",
                    "size": "1rem",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "1.3rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "2rem",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "2.75rem",
                    "name": "XL"
                },
                {
                    "slug": "xx-large",
                    "size": "3.75rem",
                    "name": "XXL"
                }
            ]
        },
        "shadow": {
            "defaultPresets": true,
            "presets": [

            ]
        },
        "dimensions": {
            "minHeight": true
        },
        "position": {
            "sticky": true
        },
        "custom": {

        }
    },
    "styles": {
        "blocks": {
            "core/paragraph": {
                "color": {
                    "text": "var(--wp--preset--color--paragraph)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "lineHeight": "2"
                }
            },
            "core/heading": {
                "color": {
                    "text": "var(--wp--preset--color--purple)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--background)",
            "text": "var(--wp--preset--color--foreground)"
        },
        "typography": {
            "fontSize": "var(--wp--preset--font-size--medium)",
            "fontFamily": "var(--wp--preset--font-family--system)",
            "lineHeight": "2"
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ]
}
